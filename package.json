{
  "name": "realm-kappa-dag-indexer",
  "version": "1.0.0",
  "description": "Index a DAG from unordered log data in a Realm database",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "run-p lint type tape",
    "lint": "eslint .",
    "tape": "node test.js",
    "posttest": "mkdir -p coverage && tap --coverage-report=text-lcov > ./coverage/lcov.info",
    "type": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Digital Democracy",
  "license": "MIT",
  "dependencies": {
    "fast-equals": "^3.0.0",
    "realm": "^10.13.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/tape": "^4.13.2",
    "@types/tmp": "^0.2.3",
    "eslint": "^8.11.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "tape": "^5.3.1",
    "tmp": "^0.2.1",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/realm-kappa-dag-indexer.git"
  },
  "bugs": {
    "url": "https://github.com/digidem/realm-kappa-dag-indexer/issues"
  },
  "homepage": "https://github.com/digidem/realm-kappa-dag-indexer#readme"
}
